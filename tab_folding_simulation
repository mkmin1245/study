      "cell_type": "code",
      "source": [
        "def cal_ab(x1,x2,y1,y2) :\n",
        "  a = (y2-y1)/(x2-x1)\n",
        "  b = (x2*y1-x1*y2)/(x2-x1)\n",
        "  return a,b"
      ],
      "metadata": {
        "id": "lDW7OmObNAVD"
      },
      "execution_count": 96,
      "outputs": []
    },
    {
      "cell_type": "code",
      "source": [
        "a, b = cal_ab(edge_x,0, 0, edge_y)"
      ],
      "metadata": {
        "id": "OiecisgkQcXc"
      },
      "execution_count": 97,
      "outputs": []
    },
    {
      "cell_type": "code",
      "source": [
        "mirror_x = (-2*a*b)/(a*a+1)\n",
        "mirror_y = 2*b/(a*a+1)"
